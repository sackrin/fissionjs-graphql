{
  "name": "schemaly-graphql",
  "version": "0.1.0",
  "description": "A library to use schemaly with graphql",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc",
    "watch": "nodemon",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sackrin/schemaly-graphql.git"
  },
  "author": "Hilary Osborne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sackrin/schemaly-graphql/issues"
  },
  "homepage": "https://github.com/sackrin/schemaly-graphql#readme",
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./test/setup.ts",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/chai-as-promised": "^7.1.0",
    "@types/jest": "^23.3.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typedoc": "^0.11.1",
    "typescript": "^3.1.2"
  },
  "dependencies": {
    "@types/graphql": "^14.0.2",
    "graphql": "^14.0.2",
    "graphql-union-input-type": "^0.4.0",
    "schemaly": "^1.3.6"
  }
}
